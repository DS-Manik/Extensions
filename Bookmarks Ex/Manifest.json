{
    "name": "BookMark Extension",
    "manifest_version": 2,
    "version": "1.0",
    "description": "bookarks manager",
    "icons": {
        "128": "3.png",
        "48": "2.png",
        "16": "1.png"
    },
    "permissions": ["bookmarks", "tabs", "activeTab"],

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "1.png"

    }
}